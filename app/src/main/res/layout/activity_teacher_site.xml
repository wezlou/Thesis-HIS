<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/video_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/menu"
    tools:context=".TeacherSite">

    <ImageView
        android:id="@+id/backteacher"
        android:layout_width="123dp"
        android:layout_height="110dp"
        android:layout_marginEnd="304dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.061"
        app:srcCompat="@drawable/img_15" />

    <ImageView
        android:id="@+id/mailbtn"
        android:layout_width="183dp"
        android:layout_height="191dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.943"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.463"
        app:srcCompat="@drawable/lightblue_plain" />

    <ImageView
        android:id="@+id/taskbtn"
        android:layout_width="183dp"
        android:layout_height="191dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.064"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.463"
        app:srcCompat="@drawable/brighter_yellow_image" />

    <ImageView
        android:id="@+id/aaa5"
        android:layout_width="218dp"
        android:layout_height="224dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.434"
        app:srcCompat="@drawable/br" />

    <ImageView
        android:id="@+id/aaa8"
        android:layout_width="218dp"
        android:layout_height="224dp"
        android:layout_marginStart="216dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.434"
        app:srcCompat="@drawable/br" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="172dp"
        android:layout_height="60dp"
        android:text="MESSAGE"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.938"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.512" />

    <TextView
        android:id="@+id/textView44"
        android:layout_width="172dp"
        android:layout_height="60dp"
        android:text="TASK"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.103"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.513" />

    <ImageView
        android:id="@+id/teachersetting"
        android:layout_width="88dp"
        android:layout_height="97dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/settingg" />

    <ImageView
        android:id="@+id/logoutbtn"
        android:layout_width="67dp"
        android:layout_height="42dp"
        app:layout_constraintTop_toBottomOf="@id/teachersetting"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/set" />

    <ImageView
        android:id="@+id/volumeOn"
        android:layout_width="67dp"
        android:layout_height="42dp"
        app:layout_constraintTop_toBottomOf="@id/logoutbtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/om" />

    <ImageView
        android:id="@+id/volumeOff"
        android:layout_width="67dp"
        android:layout_height="42dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/volumeOn"
        app:srcCompat="@drawable/mute" />
</androidx.constraintlayout.widget.ConstraintLayout>
